# Server Configuration
PORT=3000

# PostgreSQL Database Configuration (Render Database)
# ObtÃ©n estos valores desde tu dashboard de Render: Database > Connection Info
DB_HOST=tu-database.render.com
DB_PORT=5432
DB_USER=tu_usuario
DB_PASSWORD=tu_password
