# Server Configuration
PORT=3000

# PostgreSQL Database Configuration (Render)
# Obt√©n estos valores de tu base de datos en Render Dashboard
DB_HOST=dpg-xxxxx.oregon-postgres.render.com
DB_PORT=5432
DB_USER=tu_usuario
DB_PASSWORD=tu_password
DB_NAME=tu_database
DB_SSL=true

# VAPID Keys for Push Notifications
# Genera con: npx web-push generate-vapid-keys
PUBLIC_VAPID_KEY=tu_clave_publica
PRIVATE_VAPID_KEY=tu_clave_privada

# Contact Email for VAPID
VAPID_EMAIL=mailto:tu-email@gmail.com

# Environment
NODE_ENV=production
