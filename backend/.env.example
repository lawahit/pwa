# Server Configuration
PORT=3000

# Supabase Configuration
# ObtÃ©n estos valores desde tu dashboard de Supabase: https://app.supabase.com
SUPABASE_URL=https://tu-proyecto.supabase.co
SUPABASE_ANON_KEY=tu_clave_publica_supabase
SUPABASE_SERVICE_KEY=tu_clave_servicio_supabase

# PostgreSQL Database Configuration (Supabase)
# Encuentra estos valores en: Settings > Database > Connection string
DB_HOST=db.tu-proyecto.supabase.co
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=tu_password_de_supabase
DB_NAME=postgres
DB_SSL=true

# VAPID Keys for Push Notifications
# Generate keys using: npx web-push generate-vapid-keys
PUBLIC_VAPID_KEY=tu_clave_publica_vapid
PRIVATE_VAPID_KEY=tu_clave_privada_vapid

# Contact Email for VAPID
VAPID_EMAIL=mailto:tu-email@ejemplo.com
